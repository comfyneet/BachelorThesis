        <ul>
            @foreach (var a in ViewBag.Value)
            {
                <li>
                    @{
                        var tmp = (KeyValuePair<OM.Attribute, List<string>>) a;
                        @Html.Raw(tmp.Key);
                        <ul>
                        @foreach (var value in tmp.Value)
                        {
                            <li>@Html.Raw(value)</li>
                        }
                       </ul>
                      
                    }
                </li>
            }
        </ul>
    </div>
}

